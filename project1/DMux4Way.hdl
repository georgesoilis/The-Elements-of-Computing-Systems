/*
 *  4 way demultiplexor
 *
 *  sel[1]  sel[0]  ||  a   b   c   d
 *  0       0       ||  in  0   0   0
 *  0       1       ||  0   in  0   0
 *  1       0       ||  0   0   in  0
 *  1       1       ||  0   0   o   in
 *
 */
CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;
    PARTS:
        DMux(in=in, sel=sel[0], a=split0, b=split1);
        DMux(in=split0, sel=sel[1], a=a, b=c);
        DMux(in=split1, sel=sel[1], a=b, b=d);
}
